{
  "manifest_version": 3,
  "name": "Recruiter Finder (LinkedIn)",
  "version": "1.0",
  "description": "Extracts publicly visible info from LinkedIn profiles.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/in/*"],
      "js": ["content.js"]
    }
  ]
}

