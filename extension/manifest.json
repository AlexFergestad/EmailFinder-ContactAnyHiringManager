{
  "manifest_version": 3,
  "name": "Recruiter Contact Finder",
  "version": "1.0",
  "description": "Find public recruiter or career contact links for any company website.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}

